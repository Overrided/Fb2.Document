<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>Denys Pukhkyi</Authors>
    <Product />
    <PackageProjectUrl>https://github.com/Overrided/Fb2.Document</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Overrided/Fb2.Document</RepositoryUrl>
    <Company />
    <Description>.Net Standard Library for parsing *.fb2 files, which provides few different ways read files into model, and write them back into file once it needed.</Description>
    <PackageLicenseUrl></PackageLicenseUrl>
    <Copyright>2019 Denys Pukhkyi</Copyright>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>1.1.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Fb2.Document</PackageId>
    <PackageLicenseFile>License.md</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\License.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
